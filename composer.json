{
    "name": "graphjs/server",
    "type": "server",
    "description": "GraphJS REST APIs",
    "homepage": "https://graphjs.com",
    "license": "AGPL-3.0-or-later",
    "authors": [
        {
            "name": "Emre Sokullu",
            "email": "emre@phonetworks.org",
            "homepage": "http://www.emresokullu.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.3.0",
        "ext-sodium": "*",
        "ext-bcmath": "*",
        "ext-mbstring": "*",
        "ext-gd": "*",
        "phonetworks/pho-framework": "^11.6.0",
        "phonetworks/pho-server-rest": "^3.4.0",
        "pho-adapters/index-neo4j": "dev-master",
        "pho-recipes/web": "dev-master",
        "phonetworks/pho-kernel": "dev-master",
        "pho-adapters/storage-aws-s3": "dev-master",
        "pho-adapters/storage-ipfs": "dev-master",
        "vlucas/phpdotenv": "^2.4",
        "vanilla/garden-cli": "~2.0",
        "phonetworks/pho-plugins-feeds": "^0.1",
        "get-stream/stream": "~2.7",
        "mailgun/mailgun-php": "^3.0",
        "ramsey/uuid": "^3.0",
        "superbalist/php-pubsub-redis": "^2.0",
        "stripe/stripe-php": "^6.19",
        "rakit/validation": "^0.22.2",
        "php-http/guzzle6-adapter": "^1.0",
        "riverline/multipart-parser": "^2.0",
        "aws/aws-sdk-php": "^3.87",
        "pho-adapters/index-redis": "dev-master",
        "pho-recipes/network": "dev-master",
        "php-ffmpeg/php-ffmpeg": "^0.13.0",
        "danielstjules/stringy": "^3.1",
        "christoph-kluge/reactphp-http-cors-middleware": "^1.0",
        "hansott/psr7-cookies": "^2.0",
        "phonetworks/pho-lib-dht": "^1.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "fzaninotto/faker": "^1.6",
        "psy/psysh": "*",
        "squizlabs/php_codesniffer": "^2.5",
        "roave/security-advisories": "dev-master",
        "guzzlehttp/guzzle": "^6.3"
    },
    "autoload": {
	     "psr-4": {
		       "GraphJS\\": "src/GraphJS/"
	     }
    },
    "autoload-dev": {
        "psr-4": {
              "": "tests/"
        }
    },
    "config": { "optimize-autoloader": false }
}
